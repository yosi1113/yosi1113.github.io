index[start:stop:step] slice()

name = "Bro Code"

first_name = name[0//inclusive:3//exclusive]
print(first_name)

last_name = name[4:]
print(last_name)

reversed_name = name[::-1]
print(reversed_name)

website1 = "http://google.com"
website2 = "http://wikipedia.com"
slice = slice(7, -4)
print(website1[slice])
print(website2[slice])
